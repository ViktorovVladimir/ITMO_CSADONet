<Window x:Class="PostgreSQLDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PostgreSQLDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,-59,-31">
        <Button x:Name="btnLoadData" Content="Load Data" HorizontalAlignment="Left" Margin="45,96,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.222,-0.467" Click="btnLoadData_Click"/>
        <ListBox x:Name="lstMarkets" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="159,10,100,42"/>
        <Label x:Name="lblZIPcode" Content="Enter ZIP code:" HorizontalAlignment="Left" Margin="24,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtZIPcode" HorizontalAlignment="Left" Margin="18,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
